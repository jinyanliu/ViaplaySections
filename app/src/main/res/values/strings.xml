<resources>
    <string name="app_name" translatable="false">ViaplaySections</string>

    <!-- The fake title for one section provided for design reason.
   Used in drawer_list_item.xml and app_bar.xml -->
    <string name="section_title" translatable="false">SERIER</string>

    <!-- The label of section detail title in the main screen.
    Used in activity_main.xml -->
    <string name="section_title_label" translatable="true">Title:</string>

    <!-- The label of section description in the main screen.
   Used in activity_main.xml -->
    <string name="section_description_label" translatable="true">Description:</string>

    <!-- Empty view text message shows on main screen when there is neither internet, nor database data,
    witch means offline at first use.
    Used in activity_main.xml -->
    <string name="text_message_empty_view" translatable="true">Please enable internet and swipe to refresh!</string>

    <!-- The toast message to indicate the user that data is loading when the user starts the app at the first time.
    Used in MainActivity.java -->
    <string name="toast_message_data_is_loading" translatable="true">Data is loading &#8230;</string>

    <!-- The toast message to indicate the user that he wants to refresh page while offline.
    Used in MainActivity.java -->
    <string name="toast_message_offline_cannot_refresh" translatable="true">Cannot refresh page without internet !</string>
</resources>
